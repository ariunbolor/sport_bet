<title>Baseball a Sports Category Flat bootstrap responsive Website Template | Home :: w3layouts</title>
<meta name="viewport" content="width=device-width, initial-scale=1">
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<meta name="keywords" content="Baseball Responsive web template, Bootstrap Web Templates, Flat Web Templates, Andriod Compatible web template, 
Smartphone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyErricsson, Motorola web design" />
<script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
<!-- bootstrap-css -->
<link href="../../css/bootstrap.css" rel="stylesheet" type="text/css" media="all" />
<!--// bootstrap-css -->
<!-- css -->
<link rel="stylesheet" href="../../css/style.css" type="text/css" media="all" />
<link href="../../css/styles.css?v=1.6" rel="stylesheet">
<!--// css -->
<script src="../../js/jquery-1.11.1.min.js"></script>
<script src="../../js/scripts.js?v=1.7"></script>
<script src="../../js/bootstrap.js"></script>
<script type="text/javascript" src="../../js/move-top.js"></script>
<script type="text/javascript" src="../../js/easing.js"></script>
<script href='javascript/application.js'>
</script>
</head>
<body>
  <div class="main-content">
    <!-- header -->
    <div class="navigation">
      <div class="container-wrap-fluid">
          <nav class="pull">
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                <ul class="nav navbar-nav">
                  <li class="hvr-bounce-to-bottom"><a href="/" class="active">Home</a></li>
                  <li class="hvr-bounce-to-bottom"><a href="/games">Games</a></li>
                  <li><a href="/bets">My Bets</a></li>
                  <li><a href="/leaderboard">Leaderboard</a></li>
                  <li><a href="/users/">Profile</a></li>
                  <% if current_user %>
                  <li><a href="/users/logout">Log Out</a></li>
                  <% end %>
                </ul> 
                <div class="clearfix"> </div>
              </div>
          </nav>

      </div>
    </div>
    <div class="header-top">
        <div class="head-logo">
          <h1><a href="/users/login">BBettr</a></h1>
        </div>
        <div class="top-nav">
          <div class="hero fa-navicon fa-2x nav_slide_button" id="hero">
            <a href="#"><img src="../../images/menu.png" alt=""></a>
          </div>  
        </div>
        <div class="header-right">
          <div class="natalia">
              <% if current_user %>
                <a href= "/users/"><h2><%= get_points %> pts - <%= current_user.username %></h2></a>
                <% end %>
            </div>
          <div class="clearfix"> </div>
        </div>        
        <div class="clearfix"> </div>
    </div>
  </div>
  <% flash.each do |name, msg| %>
    <% if msg.is_a?(String) %>
      <div class="alert alert-<%= name == :notice ? "success" : "danger" %>">
        <a class="close" data-dismiss="alert"> × </a>
        <%= msg %>
      </div>
    <% end %>
  <% end %>

  <%= yield %>

  <!-- footer -->
    <div class="footer">
      <!-- container-wrap -->
      <div class="container-wrap">
        <div class="footer-top">
            <div class="footer-grids">
              <div class="col-md-3 f-address">
                <h5>Address</h5>
                <ul>
                  <li>128 W Hastings St #300,</li>
                  <li>Vancouver, BC V6B 1G9</li>
                  <li>Telephone: +1 888 569 6898</li>
                  <li>FAX: +1 234 567 9871</li>
                  <li>E-mail : <a href="mailto:hello@bbettr.com">hello@bbettr.com</a></li>
                </ul>
              </div>
              <div class="col-md-3 f-address f-phone">
                <h5>Phone</h5>
                <div class="f-address-text">
                  <p>+1 888 569 6898</p>
                  <p>+1 604 789 3450</p>
                </div>
              </div>
              <div class="col-md-6 f-address f-contact">
                <h5>Receive Deals</h5>
                <form>
                  <input type="text" placeholder="Email address" required="">
                  <input type="submit" value="Submit">
                </form>
              </div>
              <div class="clearfix"> </div>
            </div>
            <div class="footer-bottom">
              <div class="footer-nav">
                <ul>
                  <li><a href="index.html" class="active">Home</a></li>
                  <li><a href="/games">Games</a></li>
                  <li><a href="/bets">My Bets</a></li>
                  <li><a href="/leaderboard">Leaderboard</a></li>
                  <li><a href="/users/">Profile</a></li>
                </ul>
              </div>
              <div class="copyright">
                <p> © 2015 B-Bettr . All rights reserved | Design by Richard, Max, Ben, & Natalia</a></p>
              </div>
              <div class="clearfix"> </div>
            </div>
        </div>
      </div>
      <!-- //container-wrap -->
    </div>


  <!-- Bootstrap: Latest compiled and minified JavaScript -->
  <script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
  <script src="/javascript/application.js"></script>
  
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js" integrity="sha384-0mSbJDEHialfmuBBQP6A4Qrprq5OVfW37PRR3j5ELqxss1yVqOtnepnHVP9aJ7xS" crossorigin="anonymous"></script>
</body>

</html>
