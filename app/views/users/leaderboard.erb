
<div class="main-content">
<div class="well">
  <h3>Top 10 Users</h3>
  <table class="table table-striped">
    <tr>
      <th>Username</th>
      <th>Final Points</th>
    </tr>
    <% @top_users.each do |user| %>
      <tr>
        <td><%= user.username %></td>
        <td><%= user.final_points %></td>
      </tr>
    <% end %>
  </table>
</div>
</div>
